package src.punks {	import src.core.Game;	import src.punks.levels.Level;	public class RocketLauncher extends Launcher{				var sound_rocketLauncher:SoundRocketLauncher = new SoundRocketLauncher();						public function RocketLauncher(_shooter, _level:int = 1){			type = "rocket";			super(_shooter, _level);			cooldown = getCooldown();			hp = 100;					}		override public function behavior():void{			var rocket:Rocket;			if ( counter % cooldown == 0 && counter != 0 ){				rocket = new Rocket(this);				rocket.x = x;				rocket.y = y;				PunksGame.currentLevel.topLayer.addChild(rocket);								sound_rocketLauncher.play();			}					}				override public function getCooldown():int{			var _int;			_int = 80 - (level * 5);			return _int;		}	}}