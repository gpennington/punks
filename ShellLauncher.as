package src.punks {		import src.core.Game;	import src.punks.levels.Level;	public class ShellLauncher extends Launcher{		public function ShellLauncher(_shooter, _level:int = 1){						stop();						type = "shell";			super(_shooter, _level);						hp = 100;			cooldown = getCooldown();		}		override public function behavior():void{			gotoAndStop(1);			var shell:Shell;			if ( counter % cooldown == 0 && counter != 0){				shell = new Shell(this);				shell.x = x;				shell.y = y - 40;				PunksGame.currentLevel.middleLayer.addChild(shell);								gotoAndStop(2);			}					}				override public function getCooldown():int{			var _int;			_int = 50 - (level * 5);			return _int;		}			}}