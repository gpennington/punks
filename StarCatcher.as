package src.punks {	import src.core.Game;	import flash.display.Shape;	//import src.punks.levels.Level;	public class StarCatcher extends Launcher{				var hitTestRadius = 50;		var hitTestArea:Shape;				public function StarCatcher(_shooter, _level:int = 1){			type = "starcatcher";			//cooldown = getCooldown();									//Vacuum area			hitTestArea= new Shape();			hitTestArea.graphics.beginFill(0x3333ff, 0.1);			//hitTestArea.graphics.lineStyle( 1, 0xffffff, 0.05);			hitTestArea.graphics.drawCircle(0,0,hitTestRadius);			hitTestArea.graphics.endFill();			hitTestArea.x = x;			hitTestArea.y = y - 20;			addChild(hitTestArea);						super(_shooter, _level);						hp = 100;								}		override public function behavior():void{			for ( var e:int=0; e<PunksGame.starList.length; e++){				if ( hitTestArea.hitTestObject(PunksGame.starList[e])) {					//trace(x + "," + y);					PunksGame.starList[e].starCollect(x,y);				}			}		}		//		override public function getCooldown():int{//			var _int;//			_int = 80 - (level * 5);//			return _int;//		}	}}