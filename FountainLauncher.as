package src.punks {	import src.core.Game;	import src.punks.levels.Level;	public class FountainLauncher extends Launcher{				var isEmitting:Boolean = false;		public function FountainLauncher(_shooter, _level:int = 1){			type = "fountain";			super(_shooter, _level);						hp = 100;			cooldown = getCooldown();					}		override public function behavior():void{			if ( counter % cooldown == 0 && counter != 0){								emitToggle();							}						if ( isEmitting ){				var fountain:Spark;				for ( var s:int=0; s<3; s++){						fountain = new Spark(this,0xffffff, (Math.random() * 50) - 25, 13, 1);					fountain.x = x;					fountain.y = y - 40;					PunksGame.currentLevel.topLayer.addChild(fountain);														}			}		}		private function emitToggle(){						if ( isEmitting ){				isEmitting = false;			}			else {				isEmitting = true;			}					}				override public function getCooldown():int{			var _int;			_int = 75 - (level * 5);			return _int;		}	}}