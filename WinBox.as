package src.punks {		import flash.display.MovieClip;	import flash.events.MouseEvent;		public class WinBox extends MovieClip {						public function WinBox() {						button_levelSelect.addEventListener(MouseEvent.CLICK, levelSelectClick);			function levelSelectClick(event:MouseEvent) {				Main.mainRef.showLevelSelect();			}						button_nextLevel.addEventListener(MouseEvent.CLICK, nextLevelClick);			function nextLevelClick(event:MouseEvent) {				this.parent.loadLevel("level1");			}		}			}}