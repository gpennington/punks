package src.punks {		import src.core.Game;	import src.punks.levels.Level;	public class MaroonLauncher extends Launcher{				public function MaroonLauncher(_shooter, _level:int = 1){			stop();			type = "maroon";						super(_shooter, _level);						hp = 100;						cooldown = getCooldown();		}		override public function behavior():void{						gotoAndStop(1);						var maroon:Maroon;			if ( counter % cooldown == 0 && counter !=0 ){				maroon = new Maroon(this);				maroon.x = x;				maroon.y = y - 40;				PunksGame.currentLevel.middleLayer.addChild(maroon);									gotoAndStop(2);			}		}				override public function getCooldown():int{			var _int;			_int = 150 - (level * 15);			return _int;		}	}}