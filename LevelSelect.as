package src.punks {		import flash.display.MovieClip;	import flash.events.MouseEvent;		public class LevelSelect extends MovieClip {						public function LevelSelect() {						button_level0.addEventListener(MouseEvent.CLICK, chooseLevel);			button_level1.addEventListener(MouseEvent.CLICK, chooseLevel);			//button_level2.addEventListener(MouseEvent.CLICK, chooseLevel);						function chooseLevel(e:MouseEvent) {								switch (e.target.name){					case "button_level0":						Main.mainRef.showGame("level0");						break;					case "button_level1":						Main.mainRef.showGame("level1");						break;					//case "button_level2":						//Main.mainRef.showGame("level2");						//break;									}											}		}			}}