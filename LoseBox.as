package src.punks {		import flash.display.MovieClip;	import flash.events.MouseEvent;		public class LoseBox extends MovieClip {						public function LoseBox() {						button_levelSelect.addEventListener(MouseEvent.CLICK, levelSelectClick);			function levelSelectClick(event:MouseEvent) {				Main.mainRef.showLevelSelect();			}						button_tryAgain.addEventListener(MouseEvent.CLICK, tryAgainClick);			function tryAgainClick(event:MouseEvent) {				PunksGame.gameRef.reset();				PunksGame.gameRef.loadLevel("level1", false);//.loadLevel("level0");			}						button_menu.addEventListener(MouseEvent.CLICK, menuClick);			function menuClick(event:MouseEvent) {				Main.mainRef.showMenu();			}		}			}}